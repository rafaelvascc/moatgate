@page
@model MoatGate.Pages.User.IndexModel
@{
    ViewData["Title"] = "Users";
}
<h2>Index</h2>
<p>
    <a asp-page="Create" class="btn btn-primary"><i class="fa fa-plus"></i> Create New</a>
</p>
<table class="table table-condensed table-responsive table-striped table-hover">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].UserName)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].Email)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].EmailConfirmed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].PhoneNumber)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].PhoneNumberConfirmed)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].TwoFactorEnabled)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].LockoutEnd)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].LockoutEnabled)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoatGateIdentityUsers[0].AccessFailedCount)
            </th>
            <th>
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.MoatGateIdentityUsers)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.UserName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.EmailConfirmed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.PhoneNumberConfirmed)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TwoFactorEnabled)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LockoutEnd)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LockoutEnabled)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AccessFailedCount)
                </td>
                <td>
                    <div class="btn-group btn-group-sm">
                        <a asp-page="./Edit" asp-route-id="@item.Id" class="btn btn-sm btn-primary" title="Edit"><i class="fa fa-pencil"></i></a>
                        <a asp-page="./Delete" asp-route-id="@item.Id" class="btn btn-sm btn-danger" title="Delete"><i class="fa fa-trash-o"></i></a>
                    </div>
                </td>
            </tr>
        }
    </tbody>
</table>
